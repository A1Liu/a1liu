{
  "name": "A1Liu",
  "scripts": {
    "dev": "next dev -p 1337",
    "build": "next build",
    "export": "next export",
    "lint": "next lint --fix",
    "clean": "rm -rf .next out zig-cache zig-out"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "next": "12.1.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.34.16",
    "zustand": "3.7.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.7",
    "@types/node": "16.11.9",
    "@types/react": "17.0.35",
    "eslint": "7",
    "eslint-config-next": "12.0.3",
    "next-pwa": "5.5.2",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.0.1",
    "prettier": "2.6.2",
    "typescript": "4.4.4"
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals",
    "rules": {
      "@next/next/no-img-element": "off"
    }
  },
  "browserslist": [
    "last 2 versions",
    "> 0.25%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "postcss": {
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "postcss-preset-env": {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "nesting-rules": true,
          "custom-properties": false
        }
      }
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "quoteProps": "consistent",
    "semi": true
  }
}
