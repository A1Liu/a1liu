{
  "name": "A1Liu",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "lint": "next lint --fix",
    "deploy": "git push origin main:production"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "constate": "3.3.0",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark-prism": "1.3.6"
  },
  "devDependencies": {
    "@mdx-js/loader": "1.6.22",
    "@next/mdx": "12.0.4",
    "@types/node": "16.11.9",
    "@types/react": "17.0.35",
    "eslint": "7",
    "eslint-config-next": "12.0.4",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.0.1",
    "typescript": "4.5.2"
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals",
    "rules": {
      "semi": [
        1,
        "always"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "postcss-preset-env": {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "nesting-rules": true,
          "custom-properties": false
        }
      }
    }
  }
