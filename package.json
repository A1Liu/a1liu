{
  "name": "A1Liu",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev -p 1337",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "clean": "rm -rf .next out zig-cache zig-out"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "next": "12.1.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.34.16",
    "zustand": "3.7.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.5.0",
    "svelte": "3.44.0",
    "svelte-check": "2.2.6",
    "svelte-preprocess": "4.10.1",
    "tslib": "2.3.1",
    "typescript": "4.6.2"
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals",
    "rules": {
      "@next/next/no-img-element": "off"
    }
  },
  "browserslist": [
    "last 2 versions",
    "> 0.25%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "quoteProps": "consistent",
    "semi": true
  }
}
