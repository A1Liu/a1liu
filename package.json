{
  "name": "A1Liu",
  "scripts": {
    "start": "next start -p 1337",
    "dev": "next dev -p 1337",
    "build": "zig build -Drelease-fast && cp zig-out/lib/*.wasm public/ && next build",
    "export": "next export",
    "lint": "next lint --fix",
    "deploy": "git push origin main:production"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "next": "12.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.34.16",
    "remark-prism": "1.3.6",
    "zustand": "3.7.1"
  },
  "devDependencies": {
    "@mdx-js/loader": "1.6.22",
    "@next/bundle-analyzer": "12.0.7",
    "@next/mdx": "12.0.3",
    "@types/node": "16.11.9",
    "@types/react": "17.0.35",
    "eslint": "7",
    "eslint-config-next": "12.0.3",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.0.1",
    "typescript": "4.4.4"
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals",
    "rules": {
      "semi": [
        1,
        "always"
      ]
    }
  },
  "browserslist": [
    "last 2 versions",
    "> 0.25%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "postcss": {
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "postcss-preset-env": {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "nesting-rules": true,
          "custom-properties": false
        }
      }
    }
  }
}
