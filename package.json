{
  "name": "A1Liu",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev -p 1337",
    "zig": "watch 'zig build -p .zig/zig-out --cache-dir .zig/zig-cache && echo DONE' src/",
    "build:zig": "zig build -p .zig/zig-out --cache-dir .zig/zig-cache && echo DONE",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "typecheck": "svelte-check --tsconfig .svelte-kit/tsconfig.json",
    "check:watch": "svelte-check --tsconfig .svelte-kit/tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --no-color --write --plugin-search-dir=. src/routes src/svelte src/ts",
    "clean": "rm -rf .svelte-kit .out .zig zig-cache zig-out"
  },
  "dependencies": {},
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.34",
    "@sveltejs/kit": "1.0.0-next.348",
    "postcss": "8.4.14",
    "postcss-custom-media": "8.0.1",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-load-config": "3.1.4",
    "postcss-preset-env": "7.0.1",
    "postcss-simple-vars": "6.0.3",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.5.0",
    "svelte": "3.44.0",
    "svelte-check": "2.7.2",
    "svelte-preprocess": "4.10.1",
    "typescript": "4.6.2",
    "watch": "1.0.2"
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals",
    "rules": {
      "@next/next/no-img-element": "off"
    }
  },
  "browserslist": [
    "last 2 versions",
    "> 0.25%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "postcss": {
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "postcss-custom-media": {
        "importFrom": "src/svelte/media-queries.css"
      },
      "postcss-preset-env": {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "nesting-rules": true,
          "custom-properties": false
        }
      },
      "postcss-simple-vars": {}
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "quoteProps": "consistent",
    "semi": true
  },
  "tsconfig": {
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
      "allowJs": true,
      "checkJs": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "strict": true,
      "paths": {
        "$lib": [
          "src"
        ],
        "$lib/*": [
          "src/*"
        ],
        "@lib": [
          "src"
        ],
        "@lib/*": [
          "src/*"
        ]
      }
    }
  }
}
